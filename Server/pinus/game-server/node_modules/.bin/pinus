#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/F:/cocos/DevelopFrameworkProject/Server/pinus/game-server/node_modules/.store/pinus@1.7.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/F:/cocos/DevelopFrameworkProject/Server/pinus/game-server/node_modules/.store/pinus@1.7.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/pinus@1.7.1/node_modules/pinus/dist/bin/pinus.js" "$@"
else
  exec node  "$basedir/../.store/pinus@1.7.1/node_modules/pinus/dist/bin/pinus.js" "$@"
fi
